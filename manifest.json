{
    "name": "CurrentWeather",
    "version": "0.1",
    "options_page": "app/index.html#settings",
    "description": "Chrome extension that shows the current time and weather",
    "chrome_url_overrides": {
        "newtab": "app/index.html"
    },
    "default_locale": "en",
    "author": "Dan Covill",
    "permissions": [ 
        "storage",
        "geolocation"
    ],
    "manifest_version": 2
}